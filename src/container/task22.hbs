<main class="p-3">
    {{#with page_header}}
    <h1 class="text-center">{{title}}</h1>
    <div class="text-center" "text-muted">{{description}}</div>
    {{/with}}
    <br>
    {{#with section}}
    <div class="card">
        <div class="card-body">
            {{#with about_us}}
            <div class="text-end" "display-6">{{title_list.main}}</div>
            <h2 class="text-end">{{title_list.sub}}</h2>
            <button class="btn btn-outline-info">{{button.text}}</button>
            {{/with}}
        </div>
    </div>
    <br>
    <br>
    {{#with advantage}}
    <div class="card">
        <div class="card-body">
            <div class="display-5" "text-muted">{{title}}</div>
            <br>
            {{#with efficiency}}
            <div class="card">
                <div class="card-body">
                    <div class="display-4" "text-muted">{{title}}</div><br>
                    <h6 class="text-muted">{{sub_title}}</h6>
                    <div class="text-muted">{{description}}</div><br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>
            </div>
            {{/with}}
            <br>
            {{#with innovation}}
            <div class="card">
                <div class="card-body">
                    <div class="text-center display-5" "text-muted">{{title}}</div><br>
                    <h6 class="text-center" "text-muted">{{sub_title}}</h6>
                    <div class="text-center" "text-muted">{{description}}</div><br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>
            </div>
            {{/with}}
            <br>
            {{#with collaboration}}
            <div class="card">
                <div class="card-body">
                    <div class="text-end display-5" "text-muted">{{title}}</div><br>
                    <h6 class="text-end" "text-muted">{{sub_title}}</h6>
                    <div class="text-end" "text-muted">{{description}}</div><br>
                    <button class="btn btn-outline-warning">{{button}}</button>
                </div>
            </div>
            {{/with}}
        </div>
    </div>
    {{/with}}
    <br>
    <br>
    <div class="card">
        <div class="card-body">
            {{#with agreement}}
            <div class="display-5">{{title}}</div>
            <br>
            <div class="card">
                <div class="card-body">
                    {{#with info}}
                    <h5 class="text-muted">{{sub_title}}</h5>
                    <div class="card">
                        <div class="card-body">
                            {{#with detail}}
                            <div class="text-muted">{{text}}</div><br>
                            <button class="btn btn-success">{{button}}</button>
                            {{/with}}
                        </div>
                    </div>
                    {{/with}}
                </div>
            </div>
            {{/with}}
        </div>
    </div>

    {{/with}}

</main>